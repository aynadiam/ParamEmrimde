@model Kasa

<div class="modal fade" id="addEmployee">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title" id="addEmployeeLabel">Details Employee</h4>
                <button type="button" class="close" data-dismiss="modal">
                    <span>x</span>
                </button>
            </div>
            <div class="modal-body">
                <form asp-action="Edit">
                    <div class="form-group">
                        <label asp-for="KasaId"></label> :
                        <label asp-for="KasaId">@Model.KasaId</label>
                    </div>
                    <div class="form-group">
                        <label asp-for="KasaTarih"></label> :
                        <label asp-for="KasaTarih">@Model.KasaTarih</label>
                    </div>
                    <div class="form-group">
                        <label asp-for="KatId"></label> :
                        <label asp-for="KatId">@Model.KatId</label>
                    </div>
                    <div class="form-group">
                        <label asp-for="KasaTutar"></label> :
                        <label asp-for="KasaTutar">@Model.KasaTutar</label>
                    </div>
                    <div class="form-group">
                        <label asp-for="KasaAciklama"></label> :
                        <label asp-for="KasaAciklama">@Model.KasaAciklama</label>
                    </div>
                </form>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-primary" data-dismiss="modal">Close</button>
            </div>
        </div>
    </div>
</div>