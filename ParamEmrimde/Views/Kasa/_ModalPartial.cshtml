<!-- Add Modal Gelir -->
<div id="addGelirModal" class="modal fade">
    <div class="modal-dialog">
        <div class="modal-content">
            <form method="post" asp-controller="Kasa" asp-action="Ekle">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                    <h4 class="modal-title"><strong>Gelir Ekle</strong></h4>
                </div>
                <div class="modal-body">
                    <div class="form-group">
                        <input name="kasaTarih" class="form-control form-white" placeholder="Tarih" type="date" required>
                    </div>
                    <div class="form-group">
                        <select name="katId" class="form-control form-white" asp-items="ViewBag.KatGelir" required>
                            <option value="">Kategori Seçiniz...</option>
                        </select>
                    </div>
                    <div class="form-group">
                        <input name="kasaTutar" id="sayisalinput" class="form-control form-white" placeholder="Tutar Giriniz" required>
                    </div>
                    <div class="form-group">
                        <input name="kasaAciklama" class="form-control form-white" placeholder="Açıklama" />
                    </div>
                </div>
                <div class="modal-footer">
                    <input type="button" class="btn btn-default waves-effect" data-dismiss="modal" value="Kapat">
                    <input type="submit" class="btn btn-success save-event waves-effect waves-light" value="Ekle">
                </div>
            </form>
        </div>
    </div>
</div>
